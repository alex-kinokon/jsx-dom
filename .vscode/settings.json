{
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.rulers": [100],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist": true,
    "**/*.map": true
  },
  "cSpell.words": ["arcrole"],
  "editor.formatOnSave": true,
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false
}
