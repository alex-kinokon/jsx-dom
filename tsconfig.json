{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "sourceMap": false,
    "jsx": "react",
    "lib": [
      "dom",
      "es2015"
    ],
  },
  "exclude": [
    "node_modules",
    "coverage",
    "drafts"
  ]
}
