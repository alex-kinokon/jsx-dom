{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowJs": true,
    "importHelpers": true,
    "jsx": "react",
    "lib": ["dom", "es2015"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": false,
    "sourceMap": false,
    "target": "ES2018"
  },
  "exclude": ["node_modules", "coverage", "drafts", "alias", "lib"]
}
